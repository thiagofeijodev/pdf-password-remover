{
  "name": "pdf-password-remover",
  "version": "0.0.2",
  "homepage": "./",
  "engines": {
    "npm": "11.6.0",
    "node": "24.9.0"
  },
  "scripts": {
    "start": "node .config/rspack/rspack.dev.mjs",
    "build": "rspack --config .config/rspack/rspack.prod.mjs",
    "analyzer": "rspack build --analyze --mode development --config .config/rspack/rspack.prod.mjs",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "test": "jest --maxConcurrency=2 --maxWorkers=2",
    "test:ci": "jest --json --outputFile=test-results/jest-results.json",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "playwright test",
    "prepare": "husky"
  },
  "dependencies": {
    "@embedpdf/pdfium": "^1.5.0",
    "core-js": "^3.46.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@playwright/test": "^1.56.1",
    "@rspack/cli": "^1.5.8",
    "@rspack/core": "^1.5.8",
    "@rspack/plugin-react-refresh": "^1.5.1",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-testing-library": "^7.13.3",
    "favicons": "^7.2.0",
    "favicons-rspack-plugin": "^7.0.0",
    "file-loader": "6.2.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.4",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "react-refresh": "^0.18.0",
    "rspack-manifest-plugin": "^5.2.0",
    "workbox-webpack-plugin": "7.3.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
